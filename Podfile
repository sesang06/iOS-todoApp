# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'iOS-todoApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'SnapKit', '~> 4.0.0'
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa', '~> 4.0'
  # Pods for iOS-todoApp

  target 'iOS-todoAppTests' do
    inherit! :search_paths
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest', '~> 4.0'
    # Pods for testing
  end

  target 'iOS-todoAppUITests' do
    inherit! :search_paths
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest', '~> 4.0'
    # Pods for testing
  end

end
